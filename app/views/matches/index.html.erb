<h1>Index</h1>

<% @matches.each do |match| %>
  <ul>
    <li><%= match.id %></li>
    <li><%= match.gameboard %></li>
    <li><%= match.currentplayer %></li>
    <li><%= match.playerx_id %></li>
    <li><%= match.playero_id %></li>
    <li><%= match.outcome %></li>
    <li><%= match.winner %></li>
    <%= link_to 'Show', match_path(match) %>
    <%= link_to 'Edit', edit_match_path(match) %>
    <%= link_to 'Destroy', match_path(match),
    method: :delete, data: { confirm: 'Are you sure?' } %>
  </ul>
<% end %>
