<%= form_for @match, html: {class: 'create-gameroom-form'} do |f| %>
    <%= f.hidden_field :gameboard %>
    <%= f.hidden_field :currentplayer %>
    <%= f.hidden_field :moveindex %>
    <%= f.hidden_field :playerx_id %>
    <%= f.hidden_field :playero_id %>
    <%= f.hidden_field :outcome %>
    <%= f.hidden_field :winner %>
    <%= f.submit %>
<% end %>
